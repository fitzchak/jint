{
  "title": "Jint",
  "version": "2.8.1",

  "buildOptions": {
    "compile": [ "**/*.cs" ],
    "define": [],
    "keyFile": "Jint.snk"
  },

  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "frameworks": {
    "netstandard1.6": {
      "buildOptions": {
        "define": [ "DNXCORE50" ]
      },
      "dependencies": {
        "Microsoft.CSharp": "4.0.1",
        "NETStandard.Library": "1.6.0",
        "System.Diagnostics.Contracts": "4.0.1",
        "System.Reflection.TypeExtensions": "4.1.0"
      }
    }
  }
}
