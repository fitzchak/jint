{
  "title": "Jint Tests",
  "version": "2.8.1",

  "buildOptions": {
    "compile": [ "**/*.cs" ],
    "define": []
  },

  "testRunner": "xunit",

  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "dependencies": {
    "Jint": "2.8.1"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "buildOptions": {
        "define": [ "DNXCORE50" ]
      },
      "dependencies": {
        "xunit": "2.1.0",
        "dotnet-test-xunit": "2.2.0-preview2-build1029",

        "Microsoft.CSharp": "4.0.1",
        "System.Collections.NonGeneric": "4.0.1",

        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      },
      "imports": [
        "dotnet",
        "portable-net45+win8"
      ]
    }
  }
}
